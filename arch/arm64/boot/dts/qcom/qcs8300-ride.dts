&qupv3_id_0 {
	status = "okay";
};

&qupv3_id_1 {
	status = "okay";
};

&mdss {
	status = "okay";
};

&mdss_dp0 {
	status = "okay";
};

&mdss_dp_out {
	data-lanes = <0 1 2 3>;
	link-frequencies = /bits/ 64 <1620000000 2700000000 5400000000 8100000000>;
};

&mdss_edp_phy {
	status = "okay";
};

&spmi_bus {
	pmm8654au_2: pmic@2 {
		compatible = "qcom,pmm8654au", "qcom,spmi-pmic";
		reg = <2 SPMI_USID>;
		#address-cells = <1>;
		#size-cells = <0>;

		pmm8654au_2_gpios: gpio@8800 {
			compatible = "qcom,pmm8654au-gpio", "qcom,spmi-gpio";
			reg = <0x8800>;
			gpio-controller;
			gpio-ranges = <&pmm8654au_2_gpios 0 0 12>;
			#gpio-cells = <2>;
			interrupt-controller;
			#interrupt-cells = <2>;
		};
	};
};

&tlmm {
	bt_en_state: bt-en-state {
		pins = "gpio55";
		function = "normal";
		output-low;
		bias-pull-down;
	};

	dp_hot_plug_det: dp-hot-plug-det-state {
		pins = "gpio94";
		function = "edp0_hot";
		bias-disable;
	};

	ethernet0_default: ethernet0-default-state {
		ethernet0_mdc: ethernet0-mdc-pins {
			pins = "gpio5";
			function = "emac0_mdc";
			drive-strength = <16>;
			bias-pull-up;
		};

		ethernet0_mdio: ethernet0-mdio-pins {
			pins = "gpio6";
			function = "emac0_mdio";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	io_expander_intr_active: io_expander_intr_active {
		pins = "gpio93";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
	};

	io_expander_reset_active: io_expander_reset_active {
		pins = "gpio66";
		function = "gpio";
		drive-strength = <2>;
		bias-disable;
		output-high;
	};

	wlan_en_state: wlan-en-state {
		pins = "gpio54";
		function = "normal";
		output-low;
		bias-pull-up;
	};
};

&remoteproc_adsp {
	firmware-name = "qcom/qcs8300/adsp.mbn";
	status = "okay";
};

&remoteproc_cdsp {
	firmware-name = "qcom/qcs8300/cdsp0.mbn";
	status = "okay";
};

&remoteproc_gpdsp {
	firmware-name = "qcom/qcs8300/gpdsp0.mbn";
	status = "okay";
};

&serdes0 {
	phy-supply = <&vreg_l5a>;
	status = "okay";
};

/* HS UART Instance */
&uart2 {
	status = "okay";
	bluetooth: bluetooth {
		compatible = "qcom,wcn6855-bt";
		firmware-name = "QCA6698/hpnv21", "QCA6698/hpbtfw21.tlv";

		pinctrl-names = "default";
		pinctrl-0 = <&bt_en_state>;
		enable-gpios = <&tlmm 55 GPIO_ACTIVE_HIGH>; /* BT_EN */

		vddio-supply       = <&vreg_conn_pa>;         /* bt-vdd-ctrl1-supply */
		vddbtcxmx-supply   = <&vreg_conn_1p8>;        /* bt-vdd-ctrl2-supply */
		vddrfacmn-supply   = <&vreg_conn_1p8>;             /* bt-vdd-aon-supply, dummy */
		vddrfa0p8-supply   = <&vreg_conn_1p8>;             /* bt-vdd-aon-supply, dummy */
		vddrfa1p7-supply   = <&vreg_conn_1p8>;             /* bt-vdd-aon-supply, dummy */
		vddrfa1p2-supply   = <&vreg_conn_1p8>;             /* bt-vdd-aon-supply, dummy */
		//vddrfacmn-supply   = <&vreg_l2c>;             /* bt-vdd-aon-supply */
		//vddrfa0p8-supply   = <&vreg_l2c>;             /* bt-vdd-rfa1-supply */
		//vddrfa1p7-supply   = <&vreg_s5a>;             /* bt-vdd-rfa2-supply */
		//vddrfa1p2-supply   = <&vreg_l6e>;             /* bt-vdd-rfa3-supply */
		max-speed = <3200000>;
	};
};

&tlmm {
	pcie0_default_state: pcie0-default-state {
		perst-pins {
			pins = "gpio2";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
		};

		clkreq-pins {
			pins = "gpio1";
			function = "pcie0_clkreq";
			drive-strength = <2>;
			bias-pull-up;
		};

		wake-pins {
			pins = "gpio0";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	pcie1_default_state: pcie1-default-state {
		perst-pins {
			pins = "gpio23";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
		};

		clkreq-pins {
			pins = "gpio22";
			function = "pcie1_clkreq";
			drive-strength = <2>;
			bias-pull-up;
		};

		wake-pins {
			pins = "gpio21";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
		};
	};
};

&pcie0 {
	perst-gpios = <&tlmm 2 GPIO_ACTIVE_LOW>;
	wake-gpios = <&tlmm 0 GPIO_ACTIVE_HIGH>;

	pinctrl-names = "default";
	pinctrl-0 = <&pcie0_default_state>;

	status = "okay";
};

&pcie1 {
	perst-gpios = <&tlmm 23 GPIO_ACTIVE_LOW>;
	wake-gpios = <&tlmm 21 GPIO_ACTIVE_HIGH>;

	pinctrl-names = "default";
	pinctrl-0 = <&pcie1_default_state>;

	status = "okay";
};

&pcie0_phy {
	vdda-phy-supply = <&vreg_l6a>;
	vdda-pll-supply = <&vreg_l5a>;

	status = "okay";
};

&pcie1_phy {
	vdda-phy-supply = <&vreg_l6a>;
	vdda-pll-supply = <&vreg_l5a>;

	status = "okay";
};

&uart7 {
	status = "okay";
};

&ufs_mem_hc {
	reset-gpios = <&tlmm 133 GPIO_ACTIVE_LOW>;
	vcc-supply = <&vreg_l8a>;
	vcc-max-microamp = <1100000>;
	vccq-supply = <&vreg_l4c>;
	vccq-max-microamp = <1200000>;
	status = "okay";
};

&ufs_mem_phy {
	vdda-phy-supply = <&vreg_l4a>;
	vdda-pll-supply = <&vreg_l5a>;
	status = "okay";
};

&pmm8654au_2_gpios {
	usb1_en_state: usb1-en-state {
		pins = "gpio7";
		function = "normal";
		output-high;
		power-source = <0>;
	};
};

&usb_qmpphy {
	vdda-phy-supply = <&vreg_l7a>;
	vdda-pll-supply = <&vreg_l5a>;

	status = "okay";
};

&usb_1_hsphy {
	vdda-pll-supply = <&vreg_l7a>;
	vdda18-supply = <&vreg_l7c>;
	vdda33-supply = <&vreg_l9a>;

	status = "okay";
};

&usb_2_hsphy {
	vdda-pll-supply = <&vreg_l7a>;
	vdda18-supply = <&vreg_l7c>;
	vdda33-supply = <&vreg_l9a>;

	status = "okay";
};

&usb_1 {
	status = "okay";
};

&usb_1_dwc3 {
	snps,dis-u2-entry-quirk;
	snps,dis-u1-entry-quirk;
	snps,dis_u2_susphy_quirk;
	snps,dis_u3_susphy_quirk;

	dr_mode = "peripheral";
};

&usb_2 {
	pinctrl-names = "default";
	pinctrl-0 = <&usb1_en_state>;
	status = "okay";
};

&usb_2_dwc3 {
	dr_mode = "host";
};

&spi10 {
	status = "okay";
	spidev@0 {
		compatible = "rohm,dh2228fv";
		reg = <0>;
		spi-max-frequency = <10000000>;
		spi-cpha;
	};
};

&sdc1_clk {
	bias-disable;
	drive-strength = <16>;
};

&sdc1_cmd {
	bias-pull-up;
	drive-strength = <10>;
};

&sdc1_data {
	bias-pull-up;
	drive-strength = <10>;
};

&sdc1_rclk {
	bias-pull-down;
};

&sdhc_1 {
	vmmc-supply = <&vreg_l8a>;
	vqmmc-supply = <&vreg_s4a>;

	no-sd;
	no-sdio;
	non-removable;

	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&sdc1_clk>, <&sdc1_cmd>, <&sdc1_data>;
	pinctrl-1 = <&sdc1_clk_sleep>, <&sdc1_cmd_sleep>, <&sdc1_data_sleep>;
	status = "okay";
};
