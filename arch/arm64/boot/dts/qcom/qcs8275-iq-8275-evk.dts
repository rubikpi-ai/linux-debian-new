/delete-node/ &usb_1_dwc3;
/delete-node/ &usb_2_dwc3;
/ {
	model = "Qualcomm Technologies, Inc. IQ 8275 EVK";
	compatible = "qcom,qcs8275-iq-8275-evk", "qcom,qcs8275", "qcom,qcs8300";

	qcom,msm-id = <675 0x10000>;
