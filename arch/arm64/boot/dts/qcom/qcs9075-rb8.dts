&pmm8654au_1_gpios {
	gpio-line-names = "PMIC_C_ID0",
			  "PMIC_C_ID1",
			  "UFS1_VER_ID",
			  "IPA_PWR",
			  "",
			  "WLAN_DBU4_EN",
			  "WLAN_EN",
			  "BT_EN",
			  "USB2_PWR_EN",
			  "USB2_FAULT";

	bt_en_state: bt-en-state {
		pins = "gpio8";
		function = "normal";
		output-low;
		bias-pull-down;
	};

	usb2_en_state: usb2-en-state {
		pins = "gpio9";
		function = "normal";
		output-high;
		power-source = <0>;
	};

	wlan_en_state: wlan-en-state {
		pins = "gpio7";
		function = "normal";
		output-low;
		bias-pull-down;
	};
};

&pmm8654au_2_gpios {
	usb1_en_state: usb1-en-state {
		pins = "gpio8";
		function = "normal";
		output-high;
		power-source = <0>;
